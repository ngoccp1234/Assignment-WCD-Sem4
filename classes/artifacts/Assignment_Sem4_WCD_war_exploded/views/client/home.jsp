<!--================Home Carousel Area =================-->
<div class="c_main_title mt-5" >
    <h2 style="margin: 5% 0 3% 0; font-size: 30px">Category Ice Cream</h2>
</div>
<section class="home_carousel_area">
    <div class="home_carousel_slider owl-carousel">
        <c:forEach var="category" items="${sessionScope.categoriesClient}">
            <c:url var="products" value="/products">
                <c:param name="id" value="${category.id}"/>
            </c:url>
            <div class="item">
                <div class="h_carousel_item" >
                    <a href="${products}"><img src="${category.image}" alt="" width="400px" height="300px"></a>
                    <div class="carousel_hover">
                            <%--                        <h3>${category.name}</h3>--%>
                        <h4>${category.name}</h4>
                            <%--                        <h5>Including:</h5>--%>
                        <p>${category.description}</p>
                        <a class="discover_btn" href="${products}">view now</a>
                    </div>
                </div>
            </div>
        </c:forEach>
    </div>
</section>
<!--================End Home Carousel Area =================-->


<!--================Latest Product isotope Area =================-->
<section class="fillter_latest_product">
    <div class="container">
        <div class="c_main_title mt-3">
            <h2 style="margin: 5% 0 3% 0; font-size: 30px">Our Latest Product</h2>
        </div>
        <div class="row isotope_l_p_inner">
            <c:forEach var="latest" begin="1" end="8" items="${requestScope.productsClient}">
                <div class="col-lg-3 col-md-4 col-sm-6">
                    <div class="l_product_item">
                        <div class="l_p_img">
                            <c:url var="urlProduct" value="/product">
                                <c:param name="id" value="${latest.id}"/>
                            </c:url>
                                <%--                                <img class="img-fluid" src="<c:url value='/commom/client/assets/img/product/l-product-1.jpg'/>" alt="">--%>
                            <a href="${urlProduct}"><img class="img-fluid" src="${latest.image}" alt=""
                                                         style="height: 320px; width: 100% !important;"></a>
                            <h5 class="sale">New</h5>
                        </div>
                        <div class="l_p_text">
                            <ul>
                                <li class="p_icon"><a href="#" style="padding-top: 27%"><i
                                        class="icon_piechart"></i></a></li>
                                <li><a class="add_cart_btn" href="${urlProduct}">Detail</a></li>
                                <li class="p_icon"><a href="#" style="padding-top: 27%"><i
                                        class="icon_heart_alt"></i></a></li>
                            </ul>
                            <h4>${latest.name}</h4>
                            <h5>${latest.price}</h5>
                        </div>
                    </div>
                </div>
            </c:forEach>
        </div>
    </div>
    </div>
</section>
