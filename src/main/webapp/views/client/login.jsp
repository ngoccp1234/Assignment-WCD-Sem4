
<!--================Track Area =================-->
<section class="track_area p_100">
    <div class="container">
        <div class="track_inner">
            <div class="track_title">
                <h1>Login</h1>
            </div>
            <form class="track_form row" method="post" action="login">
                <div class="col-lg-12 form-group">
                    <label for="text">Username</label>
                    <input class="form-control" type="text" id="text" placeholder="Enter username." name="usernameUser" required>
                </div>
                <div class="col-lg-12 form-group">
                    <label for="email">Password</label>
                    <input class="form-control" type="password" id="email" placeholder="Enter password." name="passwordUser" required>
                </div>
                <div class="col-lg-12 form-group">
                    <button type="submit" value="submit" class="btn subs_btn form-control">Login</button>
                </div>
            </form>
        </div>
    </div>
</section>
<!--================End Track Area =================-->
        
